{
  "manifest_version": 3,
  "name": "Talent Land Planner",
  "version": "1.0",
  "description": "Detecta eventos en la p√°gina y permite agregarlos al calendario de Google.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://*/*"],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://talent-land.mx/*"],
      "js": ["content_script.js"],
      "run_at": "document_idle"
    }
  ]
}
